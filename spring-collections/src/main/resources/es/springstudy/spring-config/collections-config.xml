<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="subjectService" class="es.springstudy.services.SubjectService">
		<property name="subjects">
			<list>
				<ref bean="subjectMath"/>
				<ref bean="subjectHistory"/>
				<ref bean="subjectPhisical"/>
				<ref bean="subjectQuimical"/>
			</list>
		</property>
		<property name="names">
			<list>
				<value>Math</value>
				<value>History</value>
				<value>Phisical</value>
				<value>Quimical</value>
			</list>
		</property>
		<property name="setSubjects">
			<set>
				<ref bean="subjectMath"/>
				<ref bean="subjectHistory"/>
				<ref bean="subjectPhisical"/>
				<ref bean="subjectQuimical"/>
			</set>
		</property>
		<property name="mapSubject">
			<map>
				<entry key="math" value-ref="subjectMath"/>
				<entry key="history" value-ref="subjectHistory"/>
				<entry key="phisical" value-ref="subjectPhisical"/>
				<entry key="quimical" value-ref="subjectQuimical"/>
			</map>
		</property>
	</bean>
		
</beans>